{
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
        {
          "source": "/anmeldung*",
          "destination": "/anmelden",
          "type": 302
        },
        {
          "source": "/termine*",
          "destination": "/anmelden",
          "type": 302
        },
        {
          "source": "/t",
          "destination": "/anmelden",
          "type": 302
        },
        {
          "source": "/a",
          "destination": "https://zen.coderdojo.com/dojos/de/wiesbaden/wiesbaden-at-st-bonifatius",
          "type": 301
        },
        {
          "source": "/willkommen*",
          "destination": "/wir",
          "type": 302
        },
        {
          "source": "/w",
          "destination": "/wir",
          "type": 301
        },
        {
          "source": "/workshops",
          "destination": "/projekte",
          "type": 302
        },
        {
          "source": "/workshops/:page*",
          "destination": "/projekte/:page*",
          "type": 302
        },
        {
          "source": "/raeumlichkeiten*",
          "destination": "/raeume",
          "type": 302
        },
        {
          "source": "/r",
          "destination": "/raeume",
          "type": 301
        },
        {
          "source": "/anfahrt*",
          "destination": "/route",
          "type": 302
        }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=2628000"
          }
        ]
      },
      {
        "source": "**/*.@(html|css|js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=21600"
          }
        ]
      },
      {
        "source": "**/*.@(min.js|min.css|jpg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=7848000"
          }
        ]
      }
    ]
  }
}